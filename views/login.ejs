<%- include('./partials/head.ejs') %>
<main>
    <section>
        <div class="container sec-logo">
            <img src="./images/secondary-logo1.png" alt="" srcset="./images/secondary-logo1.png 1x,./images/secondary-logo2.png 2x">
        </div>
        <div id="back-nav">
            <div><a href="/">&#60;</a></div>
        </div>
        <h2 class="login-form-caption container center-text">
            Login
        </h2>

    </section>
    <section>
        <div class="container">
            <form action="/">
                <div class="form-group">
                    <div>
                        <img src="./images/username1.png" alt="">
                    </div>
                    <div>
                        <label for="user-name">username / email</label>
                        <input type="text" name="user-name">
                    </div>

                </div>
                <div class="form-group">
                    <div>
                        <img src="./images/password1.png" alt="">
                    </div>
                    <div>
                        <label for="password">password</label>
                        <input type="password" name="password">
                    </div>
                    

                </div>
                
                <button type="submit" class="form-control center-text">log in</button>
                <img src="./images/login1.png" alt="" id="login-mini-btn">
                
            
            </form>
    
        </div>
    
    </section>

               
</main>


