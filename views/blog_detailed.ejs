<%- include('./partials/head.ejs') %>
<%- include('./partials/header.ejs') %>
<main>
    <section class="story container">
        <div class="image-wrapper">
            <img src="<%= img %>" alt="<%= myPost.image_descr %>" class="story-image">
        </div>
        <div class="article">
            <h4 class="story-category"><%= myPost.category %></h4>
            <h2 class="story-title"><%= myPost.title %></h2>
            <article class="the-main">
                <%- myPost.markedBody %>
            </article>
        </div>

    </section>

</main>


<%- include('./partials/footer.ejs') %>